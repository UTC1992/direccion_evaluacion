<!-- =========================
       How it works section
============================== -->
<section id="karbar-how-it-works-section" class="how-it-works section-bg-color">
    <div class="container">
        <div class="row">
            <!-- Section main title -->
            <div class="col-xs-12 section-title-padding">
                <div class="sec-title-container text-center">
                    <div class="title-line"></div>
                    <h2 class="uppercase font-weight-7 less-mar-1">COMO UTILIZAR</h2>
                    <div class="clearfix"></div>
                    <p class="by-sub-title">Pasos para utilizar</p>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-md-11 center-block">
                <!--step 1-->
                <div class="row">
                    <div class="col-md-6 col-md-push-6 text-center"> <img src="/images/step1.png" alt="step-1"> </div>
                    <div class="col-md-6 col-md-pull-6 reveal-left-fade">
                        <div class="step-number"><span>1</span></div>
                        <h4>Coordinador</h4>
                        <p>Control de Docentes</p>
                    </div>
                </div>
                <!--step 1 end-->
                <!--step 2-->
                <div class="row">
                    <div class="col-md-6 text-center"> <img src="/images/step-2.svg" alt="step-2"> </div>
                    <div class="col-md-6 reveal-right-fade">
                        <div class="step-number"><span>2</span></div>
                        <h4>Documentos</h4>
                        <p>Coordinador genera las fichas para que llene el docente </p>
                    </div>
                </div>
                <!--step 2 end-->
                <!--step 3-->
                <div class="row">
                    <div class="col-md-6 col-md-push-6 text-center"> <img src="/images/step1.png" alt="step-3"> </div>
                    <div class="col-md-6 col-md-pull-6 reveal-left-fade">
                        <div class="step-number"><span>3</span></div>
                        <h4>Docentes</h4>
                        <p>Control de los  archivos que se encuentran dentro de las fichas</p>
                    </div>
                </div>
                <!--step 3 end-->
                <!--step 4-->
                <div class="row">
                    <div class="col-md-6 text-center"> <img src="/images/step-4.svg" alt="step-4"> </div>
                    <div class="col-md-6 reveal-right-fade">
                        <div class="step-number"><span>4</span></div>
                        <h4>Archivos PDF</h4>
                        <p>Docente genera archivos pdf de la información de cada ficha</p>
                    </div>
                </div>
                <!--step 4 end-->
                <!--step 5-->
                <div class="row">
                    <div class="col-md-6 col-md-push-6 text-center"> <img src="/images/step-4.svg" alt="step-5"> </div>
                    <div class="col-md-6 col-md-pull-6 reveal-left-fade">
                        <div class="step-number"><span>5</span></div>
                        <h4>Calificación</h4>
                        <p>Permite a los coordinadores calificar los archivos subidos por los docentes</p>
                    </div>
                </div>
                <!--step 5 end-->
            </div>
        </div>
    </div>
</section>





